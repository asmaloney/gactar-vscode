{
  "Insert basic amod template (full file)": {
    "prefix": "amod",
    "body": [
      "~~ model ~~",
      "",
      "// The name of the model (used when generating code and for error messages)",
      "name: $1",
      "",
      "// Description of the model (output as a comment in the generated code)",
      "description: '$2'",
      "",
      "// A list of authors. These are output as comments in the generated code.",
      "authors {",
      "    'Author <email@example.com>'",
      "}",
      "",
      "// Examples of starting goals to use when running the model",
      "examples {",
      "}",
      "",
      "~~ config ~~",
      "",
      "// Turn on logging by setting 'log_level' to 'min', 'info' (default), or 'detail'",
      "gactar { log_level: 'info' }",
      "",
      "// Config any modules (e.g. imaginal, extra_buffers)",
      "modules {",
      "}",
      "",
      "// Declare chunks and their layouts",
      "chunks {",
      "}",
      "",
      "~~ init ~~",
      "",
      "// Initialize the memory",
      "memory {",
      "}",
      "",
      "// Default goal",
      "goal []",
      "",
      "~~ productions ~~",
      "",
      "// Name of the production",
      "production {",
      "    description: 'Optional description of production'",
      "    ",
      "    // Buffers to match",
      "    match {",
      "        goal []",
      "    }",
      "    // Steps to execute",
      "    do {",
      "    }",
      "}"
    ],
    "description": "Insert basic amod template"
  },
